# Makefile

UID1=704534585
UID2=604721196
UID3=104778067
PROJ=Project2

.PHONY: dist clean run

run:
	./run_all.sh "question_*.py"
	
dist:
	zip -X $(PROJ)_$(UID1)_$(UID2)_$(UID3).zip *.py Project*Report.pdf *.csv README Makefile run_all.sh

# removes all files generated by make and vim's tmp files
clean:
	rm -f *.pyc *~ *.zip

clobber: clean
	rm -f *.pdf *.png
